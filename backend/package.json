{
  "type": "module",
  "scripts": {
    "dev": "node --watch -r dotenv/config src/app.js dotenv_config_path=../.env",
    "start": "node -r dotenv/config src/app.js dotenv_config_path=../.env",
    "seed": "npx prisma db seed",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:studio": "dotenv -e ../.env -- prisma studio",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/sensible": "^6.0.1",
    "@graphql-tools/load": "^8.0.9",
    "@graphql-tools/schema": "^10.0.13",
    "@prisma/client": "^6.0.1",
    "altair-fastify-plugin": "^8.0.5",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^7.4.1",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "fastify-socket.io": "^5.1.0",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "mercurius": "^15.1.0",
    "pino-pretty": "^10.3.1",
    "socket.io": "^4.8.1",
    "vue3-lottie": "^3.3.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1"
  },
  "prisma": {
    "seed": "node ./db/seed.js",
    "schema": "./db/schema.prisma"
  }
}
